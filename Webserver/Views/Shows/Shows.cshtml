@model IEnumerable<WebApiClient.DTOs.ShowDto>

@{
    ViewData["Title"] = "Shows";
}

<h1>Shows</h1>


<table class="table table-hover table-bordered bg-grey text-light">
    <thead>
        <tr>
            <th>
                Starttime
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StartTime)
                </td>
            </tr>
        }
    </tbody>
</table>
